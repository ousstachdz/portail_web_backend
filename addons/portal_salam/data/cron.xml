<odoo>
    <data noupdate="1">

        <record id="ir_cron_crm_lead_http_request" model="ir.cron">
            <field name="name">CRM Lead HTTP Request - Every 5 Minutes</field>
            <field name="model_id" ref="portal_salam.model_crm_lead_cron" />
            <field name="state">code</field>
            <field name="code">model.send_http_request()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>

    </data>
</odoo>