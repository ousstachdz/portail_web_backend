<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1" >
        <!-- List signatures -->
        <record id="signature_1" model="wk.signature">
            <field name="position">رئيس قطاع الخزينة و العمليات المالية المحلية و الدولية</field>
            <field name="position_comite">عضو</field>
        </record>

         <record id="signature_2" model="wk.signature">
            <field name="position">مدير النشاط التجاري للمؤسسات</field>
            <field name="position_comite">عضو</field>
        </record>

         <record id="signature_3" model="wk.signature">
            <field name="position">مدير إدارة الدراسات الائتمانية للمؤسسات</field>
            <field name="position_comite">عضو</field>
        </record>

         <record id="signature_4" model="wk.signature">
            <field name="position">رئيس خلية إدارة المخاطر</field>
            <field name="position_comite">عضو</field>
        </record>

         <record id="signature_5" model="wk.signature">
            <field name="position">نائب المدير العام</field>
            <field name="position_comite">عضو</field>
        </record>

           <record id="signature_6" model="wk.signature">
            <field name="position">المدير العام</field>
            <field name="position_comite">رئيس اللجنة</field>
        </record>
       
        <!-- list configuration-signatures (multi-signatures) -->
        <record id="multi_signature_1" model="wk.multi.signature">
        <field name="reference">افتراضي</field>
        <field name="signature_ids" eval="[(6, 0, [
                                          ref('signature_1'), 
                                          ref('signature_2'), 
                                          ref('signature_3'), 
                                          ref('signature_4'), 
                                          ref('signature_5'), 
                                          ref('signature_6')
                                            ])]"/>
        </record>

        <!-- <record id="default_multi_signature" model="ir.config_parameter">
            <field name="key">wk.default_signature</field>
            <field name="value" eval="ref('multi_signature_1')"/>
        </record> -->






    </data>

  





</odoo>