    <t t-call="web.html_container">
    <head>
            <meta charset="UTF-8"/>
            <style>
                div{
                    width:100%;
                }
                table{
                    width:100%;
                    direction: rtl;
                    table-layout: fixed;
                }
                body {
                    direction: rtl;
                    font-family: 'NotoArabic-Regular';
                }
                td, th {
                    text-align: center;  /* Centers the text horizontally */
                    vertical-align: middle;  /* Centers the text vertically */
                }
                .reversed-table > tbody > tr > td {
